set nocompatible

set encoding=utf-8
set fileencoding=utf-8

syntax enable
set showmatch

set incsearch
set ignorecase

set showmode
set showcmd
set modeline
set ruler
set title
set number
set relativenumber

map <Up> <Nop>
imap <Up> <Nop>
map <Down> <Nop>
imap <Down> <Nop>
map <Left> <Nop>
imap <Left> <Nop>
map <Right> <Nop>
imap <Right> <Nop>
